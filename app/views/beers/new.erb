<h1>CREATE A NEW BEER</h1>

<%= form_for @beer do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :category %>
<%= f.text_field :category %>
<%= f.label :abv %>
<%= f.text_field :abv %>
<%= f.label :info %>
<%= f.text_field :info %>
<%= f.label :image %>
<%= f.text_field :image %>
<%= f.label :ratings %>
<%= f.number_field :ratings %>
<%= f.submit %>
<% end %>
